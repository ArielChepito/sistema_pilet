 se.codi_reso_soli = e.codi_reso_soli and u.codi_usua = s.codi_usua and uu.codi_usua = s.codi_enca and
 d.codi_depa = s.codi_depa and s.esta_soli = 4 and MONTH(s.fech_hora_soli)=$P{mes}
